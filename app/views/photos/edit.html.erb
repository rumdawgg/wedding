<div class="center jumbotron">
    <%= image_tag @photo.file.url() %>
<div class="center jumbotron">
<%= form_for @photo do |f| %>
  <%= render 'shared/errors', object: @photo %>

  <div class="form-group">
    <%= f.label :caption %>
    <%= f.text_field :caption, class: 'form-control' %>
  </div>

   <div class="form-group">
    <%= f.check_box :delete_asset, class: 'form-control' %>
  </div>


  <%= f.submit 'Update', class: 'btn btn-primary' %>
<% end %>
</div>
</div>