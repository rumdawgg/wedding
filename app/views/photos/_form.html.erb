<%= form_for(@photo) do |f| %>

  <%= f.label :title %>
  <%= f.text_field :title, class: 'form-control' %>

  <%= f.label :caption %>
  <%= f.text_field :caption, class: 'form-control' %>

  <%= f.label :file %>
  <%= f.file_field :file, class: 'js-uploader' %>

  <%= f.submit "Upload Photo", class: "btn btn-primary" %>
<% end %>