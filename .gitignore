# Ignore bundler config.
/.bundle

# Ignore files that could contain secrets or passwords
config/database.yml
config/secrets.yml
config/aws.yml

# Ignore all logfiles and tempfiles.
/log/*
!/log/.keep
/tmp
*.swp
*.swo
.rvmrc
.rbenv-version
.DS_Store
.rbenv-vars
*.sublime-*
*sublime-*f

# don't commit images to the repository
*.jpg

/public/assets/
/public/source_maps/
